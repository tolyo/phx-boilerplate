{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["ES2018", "DOM"],
    "target": "ES5",
    "sourceMap": false,
    "declaration": false,
    "importHelpers": true,
    "incremental": true,
    "typeRoots": ["@types/"]
  },
  "include": ["./**/*.js", "./**/*.ts"],
  "exclude": []
}
